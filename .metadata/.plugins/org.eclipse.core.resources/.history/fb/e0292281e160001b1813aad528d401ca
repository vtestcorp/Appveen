package com.PageObjects;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class ObjectCreateDataService extends ObjectLoginAuthor {

	public ObjectCreateDataService() {
		super();
	}
	String NoOfAttributes="1";
	By editButton=By.xpath("//span[text()='Edit']");
	By viewButton=By.xpath("//span[normalize-space()='View']");
	By newAttribute=By.xpath("//span[contains(text(),'New Attribute')]");
	By addAttribute=By.xpath("(//input[@type='text'])["+NoOfAttributes+""]");
}
