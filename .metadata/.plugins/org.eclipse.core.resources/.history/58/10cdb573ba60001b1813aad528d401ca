package com.PageModules;

import java.io.IOException;

import org.openqa.selenium.support.PageFactory;

import com.Config.BaseClass;
import com.PageObjects.ObjectLogin;

public class LaunchAuthor extends BaseClass {
	ObjectLogin login=new ObjectLogin();
	BaseClass base=new BaseClass();
	
	public LaunchAuthor() {
		super();
		PageFactory.initElements(getDriver(), this);
	}
	public void launchAuthor() {
		try {
			base.launchAuthor();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	public void loginAuthor() {
		
		enterText(login.username, "appadmin_demo@user.com");
		clickElement(login.nextButton);
		enterText(login.password, "OpenSesame");
		clickElement(login.loginButton);
		System.out.println("Logged in successfully");
	}
}
